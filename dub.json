	"description": "D bindings for the memgraph DB",
	"license": "MIT",
	"name": "memgraph-d",
	"preGenerateCommands": [
		"$PACKAGE_DIR/pregen"
	],
	"libs": [ "openssl" ],
	"configurations": [
		{
			"name": "lib",
			"targetType": "staticLibrary",
			"lflags": [ "$PACKAGE_DIR/mgclient/build/src/libmgclient.a" ]
		},
		{
			"name": "shared",
			"targetType": "dynamicLibrary",
			"libs": [ "mgclient" ],
			"lflags": [ "-L$PACKAGE_DIR/mgclient/build/src" ]
		}
	]
}
